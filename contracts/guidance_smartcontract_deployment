# Deploy ke testnet (default)
npx hardhat run --network polygonAmoyTestnet scripts/deploy_x402.js

# Estimasi gas
npx hardhat run --network polygonAmoyTestnet scripts/estimate_gas_cost.js

# (Nanti jika sudah un-comment blok mainnet)
npx hardhat run --network polygonMainnet scripts/deploy_x402.js



// Deploy testnet:
docker compose run --rm x402-deployer npx hardhat run --network megaTestnet scripts/deploy_x402.js


 // Deploy mainnet:
docker compose run --rm x402-deployer npx hardhat run --network polygonMainnet scripts/deploy_x402.js
